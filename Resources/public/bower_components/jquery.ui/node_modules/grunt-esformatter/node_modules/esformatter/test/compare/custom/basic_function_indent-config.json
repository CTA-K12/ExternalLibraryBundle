            "FunctionDeclarationClosingBrace" : false,
            "ReturnStatement" : false
        },
        "after" : {
            "FunctionDeclarationOpeningBrace" : false,
            "FunctionDeclarationClosingBrace" : false
        }
    },
    "whiteSpace" : {
